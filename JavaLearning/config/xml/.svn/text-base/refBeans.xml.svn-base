<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN/EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="RefBean" class="op.sample.spring.refbean.DataPrefixGenerator">
		<property name="Pattern" value="yyyy/MM/dd"/>
	</bean>
	
	<bean id="SequenceGenerator" class="op.sample.spring.refbean.SequenceGenerator">
		<property name="suffix" value="_suffix"/>
		<property name="PrefixGenerator">
			<ref bean="RefBean"/>
		</property>
	</bean>
</beans>